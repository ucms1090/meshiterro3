<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_user_image(100,100) %>
</div>

<% @posts.each do |postdata| %>
<div>
  <p>ユーザー画像：<%=  image_tag postdata.user.get_user_image(100,100) %></p>
  <p>ショップ名：<%= postdata.shop_name %></p>
  <p>説明：<%= postdata.caption %></p>
  <p>ユーザーネーム：<%= postdata.user.name %></p>
</div>
<% end %>